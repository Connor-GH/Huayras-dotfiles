#!/bin/sh
[DISK]
command=~/.config/i3blocks/scripts/newdisk
interval=300
color=#C9E3DB
[CPU]
command=~/.config/i3blocks/scripts/newcpuavg
interval=2
color=#00B4EB
[MEM]
command=~/.config/i3blocks/scripts/newmem
interval=3
color=#FEC925
[SOUND]
full_text= :  0% 
command=~/.config/i3blocks/scripts/sound_info.sh
interval=3
[TIME_DATE]
command=~/.config/i3blocks/scripts/newtime
interval=60
